<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kiosk.Wpf"
        xmlns:Wpf="clr-namespace:DotNetBrowser.Wpf;assembly=DotNetBrowser.Wpf" x:Class="Kiosk.Wpf.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" WindowStyle="None"
        WindowState="Maximized"
        Topmost="True" ResizeMode="NoResize"
        ShowInTaskbar="False" Closed="MainWindow_OnClosed">
    <Grid>
        <Wpf:BrowserView Name="BrowserView" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
